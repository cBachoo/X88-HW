noLayer: true
board:
    color:
        fr4: '#000000' #Substrate
        cu: '#cccccc' #Copper
        cf: '#cc9933' #Copper (finished)
        sm: "#000000aa" #Soldermask
        ss: '#ffffff' #Silkscreen
        sp: '#999999' #Solderpaste
        out: "rgba(00,00,128,0.75)" #Board outline
layer:
    X88C-EdgeCuts.gbr:
        type: outline
        side: all
        options:
            plotAsOutline: 0.05
    X88C-MaskBottom.gbr:
        type: soldermask
    X88C-MaskTop.gbr:
        type: soldermask
    X88C-PasteBottom.gbr:
        type: solderpaste
    X88C-PasteTop.gbr:
        type: solderpaste
    X88C-SilkBottom.gbr:
        type: silkscreen
    X88C-SilkTop.gbr:
        type: silkscreen
    X88C-NPTH-drl_map.pdf:
        type: drawing

out:
    images\